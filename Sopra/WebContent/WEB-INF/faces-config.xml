<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
 <managed-bean>
  <managed-bean-name>userBean</managed-bean-name>
  <managed-bean-class>model.account.UserBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>moderstellungsService</property-name>
   <property-class>model.ModulErstellenBean</property-class>
   <value>#{modulErstellenBean}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>user</managed-bean-name>
  <managed-bean-class>klassenDB.User</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>modelBean</managed-bean-name>
  <managed-bean-class>model.ModelBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>modulHandbuchBean</managed-bean-name>
  <managed-bean-class>model.ModulHandbuchBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>loeschBean</managed-bean-name>
  <managed-bean-class>model.account.LoeschBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean eager="true">
  <managed-bean-name>modulErstellenBean</managed-bean-name>
  <managed-bean-class>model.ModulErstellenBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>baumstrukturBean</managed-bean-name>
  <managed-bean-class>model.BaumstrukturBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>modulhandbuchBaumstrukturBean</managed-bean-name>
  <managed-bean-class>model.ModulhandbuchBaumstrukturBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>benutzerAendernBean</managed-bean-name>
  <managed-bean-class>model.account.BenutzerAendernBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>benutzerAnlegenBean</managed-bean-name>
  <managed-bean-class>model.account.BenutzerAnlegenBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>pdfBean</managed-bean-name>
  <managed-bean-class>model.PdfBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>rechteverwaltungsBean</managed-bean-name>
  <managed-bean-class>model.account.RechteverwaltungsBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>modulsuche</from-outcome>
   <to-view-id>/modulsuche.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>modulLoeschen</from-outcome>
   <to-view-id>/modulLoeschen.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>modulErstellen</from-outcome>
   <to-view-id>/modulErstellen.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>benutzerAnlegen</from-outcome>
   <to-view-id>/benutzerAnlegen.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>benutzerdatenAendern</from-outcome>
   <to-view-id>/benutzerdatenAendern.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>pdfSeite</from-outcome>
   <to-view-id>/WebContent/pdf.pdf</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>rechteverwaltung2</from-outcome>
   <to-view-id>/rechteverwaltung2.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>rechteverwaltung1</from-outcome>
   <to-view-id>/rechteverwaltung1.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
